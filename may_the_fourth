SELECT * FROM may_the_fourth;

SELECT * FROM may_the_fourth ORDER BY score;

SELECT * FROM may_the_fourth ORDER BY year;

SELECT * FROM may_the_fourth WHERE minutes > 150;

SELECT * FROM may_the_fourth WHERE score > 7 AND minutes < 130 ORDER BY score DESC;
